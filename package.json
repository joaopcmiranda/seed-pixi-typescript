{
  "name": "pixi-webpack-ts",
  "version": "6.0.4",
  "license": "CC-BY-NC-4.0",
  "author": "Lo√Øc TRUCHOT <loic.truchot.pro@gmail.com>",
  "description": "A pre installed environment to work with pixi.js with webpack & typescript",
  "main": "src/app/App.ts",
  "scripts": {
    "npm-reset": "rm -rf node_modules && npm cache verify && npm i",
    "git-reset": "git reset HEAD --hard && git clean -fd && git fetch --all --tags --prune && git pull",
    "start": "webpack serve",
    "build": "webpack",
    "dependency-graph": "npm run --silent _dependency-graph",
    "_dependency-graph": "npx depcruise src/$npm_config_root $npm_config_args --config --output-type dot | dot -T svg > ./dependency-graph.svg"
  },
  "keywords": [
    "pixijs",
    "playground",
    "basic",
    "webpack",
    "typescript",
    "seed"
  ],
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "mobx": "^6.6.2",
    "pixi.js": "^6.5.6",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dependency-cruiser": "^11.16.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.26.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
